{
  "title": "CCPorted Documentation",
  "description": "Documentation for the CCPorted website and its JavaScript components",
  "author": "CCPorted Team",
  "language": "en",
  "structure": {
    "readme": "README.md",
    "summary": "SUMMARY.md"
  },
  "plugins": [
    "search",
    "theme-default",
    "expandable-chapters",
    "code",
    "copy-code-button",
    "github",
    "edit-link"
  ],
  "pluginsConfig": {
    "github": {
      "url": "https://github.com/ccported/ccported.github.io"
    },
    "edit-link": {
      "base": "https://github.com/ccported/ccported.github.io/edit/typescript-annotations",
      "label": "Edit This Page"
    }
  },
  "links": {
    "sidebar": {
      "CCPorted Website": "https://ccported.github.io",
      "GitHub Repository": "https://github.com/ccported/ccported.github.io"
    }
  }
}
